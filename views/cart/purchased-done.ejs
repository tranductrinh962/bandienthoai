<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>thêm vô giỏ hàng thành công</title>
    <link rel="shortcut icon" href="../../images/favicon.png" type="image/x-icon">

    <!-- all css -->
    <link rel="stylesheet" href="../../css/bootstrap.min.css">
    <link rel="stylesheet" href="../../css/jquery-ui.css">
    <link rel="stylesheet" href="../../css/slick.css">
    <link rel="stylesheet" href="../../css/line-awesome.css">
    <link rel="stylesheet" href="../../css/nice-select.css">
    <link rel="stylesheet" href="../../css/style.css">
    <link rel="stylesheet" href="../../css/responsive.css">
</head>

<body>
 <%- include('../partials/navibar.ejs') %>

    <!-- breadcrumbs -->
    <div class="container">
        <div class="breadcrumbs">
            <a href="/"><i class="las la-home"></i></a>
            <a href="/" class="active">Thêm vô giỏ hàng thành công</a>
        </div>
    </div>

    <!-- order complete -->
    <div class="cart_area section_padding_b">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="row">
                        <div class="col-lg-8 mx-auto">
                            <div class="order_complete">
                                <div class="complete_icon">
                                    <img loading="lazy" src="../../images/complete.png" alt="success">
                                </div>
                                <div class="order_complete_content">

                                    <h4>Bạn đã thêm sản phẩm thành công !</h4>
                                    <div class="order_complete_btn">
                                        <a href="/products/<%=productId%>" class="default_btn rounded">Tiếp tục mua sắm</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>



    <!-- all js -->
    <script src="../../js/bootstrap.bundle.min.js"></script>
    <script src="../../js/jquery-3.5.1.min.js"></script>
    <script src="../../js/jquery-ui.min.js"></script>
    <script src="../../js/slick.min.js"></script>
    <script src="../../js/jquery.nice-select.min.js"></script>
    <script src="../../js/app.js"></script>
</body>



</html>