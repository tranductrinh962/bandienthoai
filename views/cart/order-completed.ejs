        <div class="breadcrumbs">
            <a href="/"><i class="las la-home"></i></a>
            <a href="/" class="active">Đơn hàng đã đặt thành công</a>
        </div>
    </div>

    <!-- order complete -->
    <div class="cart_area section_padding_b">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <div class="row">
                        <div class="col-lg-8 mx-auto">
                            <div class="order_complete">
                                <div class="complete_icon">
                                    <img loading="lazy" src="../../images/complete.png" alt="success">
                                </div>
                                <div class="order_complete_content">

                                    <h4>Đơn hàng của bạn đã được hoàn thành!</h4>
